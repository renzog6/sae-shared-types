{
  "name": "@sae/shared-types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for SAE projects (backend + frontend)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate:openapi": "openapi-typescript http://localhost:3305/api/docs-json --output src/openapi-types.ts",
    "generate:zod": "openapi-zod-client http://localhost:3305/api/docs-json -o src/zod-types.ts",
    "generate:all": "npm run generate:openapi && npm run generate:zod",
    "build": "tsc"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "openapi-zod-client": "^1.16.0",
    "typescript": "^5.6.3"
  },
  "author": "SAE Team",
  "license": "MIT"
}
